{
  "name": "@geospoc/gqi-mapper-documentation",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,ts,vue,md}\" --ignore-path .eslintignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,ts,vue,md}\" --ignore-path .eslintignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,ts,vue,md}\" --ignore-path .prettierignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,ts,vue,md}\" --ignore-path .prettierignore",
    "prepare": "husky install",
    "release": "shipjs prepare",
    "release:dry": "shipjs prepare --dry-run",
    "release:auto": "shipjs prepare --yes"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "docus": "0.9.0",
    "@nuxt/typescript-build": "^2.1.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nuxt/image": "^0.6.1",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/node": "^17.0.21",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "husky": "^7.0.4",
    "nuxt-compress": "^5.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "shipjs": "^0.24.2"
  },
  "author": {
    "name": "GeoSpoc Dev Team",
    "email": "developers@geospoc.com",
    "url": "https://geospoc.io"
  },
  "contributors": [
    {
      "name": "Vinayak Kulkarni",
      "email": "inbox.vinayak@gmail.com",
      "url": "https://vinayakkulkarni.dev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geospoc/unc-sch-documentation.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geospoc/unc-sch-documentation/issues"
  },
  "homepage": "https://github.geospoc.io/unc-sch-documentation"
}
