{
  "name": "docus-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|static|dist)/**/}*.{js,vue}\""
  },
  "dependencies": {
    "docus": "^0.0.9",
    "nuxt": "^2.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.1",
    "prettier": "^2.2.1"
  },
  "author": {
    "name": "GeoSpoc Dev Team",
    "email": "developers@geospoc.com",
    "url": "https://geospoc.io"
  },
  "contributors": [
    {
      "name": "Vinayak Kulkarni",
      "email": "inbox.vinayak@gmail.com",
      "url": "https://vinayakkulkarni.dev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geospoc/unc-sch-documentation.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geospoc/unc-sch-documentation/issues"
  },
  "homepage": "https://github.geospoc.io/unc-sch-documentation"
}
